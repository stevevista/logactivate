    "build": "gulp",
    "test": "jest",
    "nexe:x64": "nexe dist/index.js -tx64-8.12.0 -o logactivate-linux-x64-1.0.0",
    "nexe:win32": "nexe dist/index.js -twin32-8.12.0 -o logactivate-win32-1.0.0.exe",
    "nexe": "npm run build && npm run nexe:x64 && npm run nexe:win32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevevista/logactivate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevevista/logactivate/issues"
  },
  "homepage": "https://github.com/stevevista/logactivate#readme",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/tests/**/*.+(js)"
    ]
  },
  "dependencies": {
    "express": "^4.16.3",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "multer": "^1.4.0",
    "sequelize": "^4.39.0"
  },
  "devDependencies": {
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-eslint": "^5.0.0",
    "gulp-terser": "^1.1.5",
    "jest": "^23.6.0",
    "nexe": "^2.0.0-rc.34",
    "supertest": "^3.3.0"
  }
